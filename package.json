{
  "name": "orgdo",
  "version": "0.2.0",
  "description": "Command-line tool to manage the Todo lists",
  "bin": "dist/bin/orgdo.js",
  "scripts": {
    "lint": "tslint -p tsconfig.json --fix",
    "build": "tsc",
    "build:prod": "tsc -p tsconfig.prod.json",
    "prepublish": "npm run clean && npm run build:prod",
    "test": "jest",
    "clean": "rimraf dist"
