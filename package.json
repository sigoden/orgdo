    "lodash": "^4.17.11",
    "lowdb": "^1.0.0",
    "mkdirp": "^0.5.1",
    "node-ipc": "^9.1.1",
    "node-notifier": "^5.3.0",
    "timestring": "^5.0.1",
    "update-notifier": "^2.5.0",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/node-ipc": "^9.1.1",
    "jest": "^23.6.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4"
  }
